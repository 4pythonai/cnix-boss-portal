{
    "[js]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "eslint.alwaysShowStatus": true,
    "eslint.format.enable": true,
    "eslint.packageManager": "npm",
    "eslint.run": "onSave",
    "eslint.validate": ["javascript", "javascriptreact"],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.formatOnPaste": false,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "files.eol": "\n",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#1f287d",
        "activityBar.activeBorder": "#fa45d4",
        "activityBar.background": "#1f287d",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "sash.hoverBorder": "#1f287d",
        "statusBar.background": "#151b54",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#1f287d",
        "statusBarItem.remoteBackground": "#151b54",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#151b54",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#151b5499",
        "titleBar.inactiveForeground": "#e7e7e799",
        "editorGroup.border": "#1f287d",
        "panel.border": "#1f287d",
        "sideBar.border": "#1f287d",
        "statusBar.border": "#151b54",
        "statusBar.debuggingBackground": "#544e15",
        "statusBar.debuggingBorder": "#544e15",
        "statusBar.debuggingForeground": "#e7e7e7",
        "tab.activeBorder": "#1f287d",
        "titleBar.border": "#151b54",
        "commandCenter.border": "#e7e7e799"
    },
    "peacock.color": "#151B54",
    "terminal.explorerKind": "external",
    "editor.fontSize": 12
}
